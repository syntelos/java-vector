<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <title>Vector XS Error</title>
    <style type="text/css">
      body {
        background-color: #FFF;
        font-family: sans-serif;
      }
      h1, p, pre {
        color: #000;
        margin-left: 40px;
      }
      h1 {
        margin-top: 120px;
      }
      pre {
        border: 1px solid #000;
      }
    </style>
  </head>
  <body>
    {{#error}}
    <h1>{{=error_status}}</h1>
    <p> {{=error_message}} </p>
    <pre>{{=error_exception}}</pre>
    {{/error}}
  </body>
</html>
